page{
    background: #f2f2f2;
}

.goods-main{

}
.goods-main-item{
    height:190rpx;
    box-shadow:0 2rpx 0 0 rgba(35,24,21,0.1);
    display: flex;
    align-items: center;
    padding: 0 20rpx;
    font-size: 28rpx;
    color: #333333;
    background: white;
}

.goods-main-item image{
    width: 130rpx;
    height: 130rpx;
}
.goods-main-item .goods-main-info{
    width: 400rpx;
    margin-left: 20rpx;
}

.goods-main-item .goods-main-info>view{
    line-height: 60rpx;
}

.goods-main-item .goods-number{
    width: 175rpx;
    text-align: right;
}

.goods-main-item .goods-number text{
    color: #595CA1;
}