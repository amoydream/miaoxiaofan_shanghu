.container{
    height: 100%;
    background:#f2f2f2;
    font-size: 30rpx;
}
.info{
    width: 100%;
    line-height: 100rpx;
    text-align: center;
    padding: 20rpx;
    box-sizing: border-box;
}
.container navigator{
    padding:0 30rpx;
    box-sizing: border-box;
    line-height: 80rpx;
    font-size:28rpx;
    font-family:MicrosoftYaHei;
    font-weight:400;
    color:rgba(51,51,51,1);
    background: white;
    border-bottom: 1rpx solid #f2f2f2;
}
.container .operating{
    margin-top: 1px;
    margin-bottom: 20rpx;
}

.container navigator .right{
    margin-top: 30rpx;
    width: 20rpx;
    height: 20rpx;
    float: right;
}


.container navigator .money{
    width: 45rpx;
    height: 46rpx;
    vertical-align: middle;
}

.container navigator .set{
    width: 47rpx;
    height: 46rpx;
    vertical-align: middle;
}

.container navigator .password{
    width: 47rpx;
    height: 46rpx;
    vertical-align: middle;
}

.capacity{
    width:750rpx;
    height:88rpx;
    background:rgba(89,92,161,1);
    font-size:28rpx;
    color:rgba(255,255,255,1);
    text-align: center;
    line-height: 88rpx;
    position: fixed;
    bottom: 0;
}
