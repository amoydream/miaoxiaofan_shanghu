.container {
    min-height: 100%;
    background: #F2F2F2;
    padding: 20rpx;
    box-sizing: border-box;
}

.test_switch{
    width:710rpx;
    height:160rpx;
    background:rgba(255,255,255,1);
    border-radius:8rpx;
    margin: 0 auto;
    margin-bottom: 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.test_switch image{
    width: 120rpx;
    height: 120rpx;
}

.test_switch text{
    font-size: 30rpx;
    margin-left: 20rpx;
    flex: 2;
}

.test_switch .btn{
    width:86rpx;
    height:46rpx;
    background:#CEB267;
    border-radius:23rpx;
    position: relative;
}

.test_switch .btn:after{
    content: '';
    position: absolute;
    width:42rpx;
    height:42rpx;
    background:rgba(255,255,255,1);
    border-radius:50%;
    right: 2rpx;
    top: 2rpx;
}

.test_switch .btn.active{
    background: #c3c3c3;
}

.test_switch .active:after{
    left: 2rpx;
}

.test_main{
    background: white;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.test_main .test_main_title{
    font-size:30rpx;
    font-weight:bold;
    color:rgba(51,51,51,1);
    line-height: 88rpx;
}

.test_main .test_main_item{
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 30rpx;
    height: 88rpx;
    box-shadow:0 1px 0 0 rgba(0, 0, 0, 0.1);
}

.test_main .test_main_item image{
    width: 20rpx;
    height: 30rpx;
}

.test_main .test_main_item:last-child{
    box-shadow: none;
}