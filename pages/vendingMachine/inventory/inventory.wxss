page{
    background: #f2f2f2;
    padding-top: 20rpx;
    box-sizing: border-box;
}
.head{
    width: 710rpx;
    height: 180rpx;
    background: white;
    margin: 0 auto;
    position: relative;
}

.head image{
    width: 120rpx;
    height: 120rpx;
    border: 1px solid #000;
    border-radius: 50%;
    margin-left: 23rpx;
    margin-top: 35rpx;
}

.head>view{
    font-size:28rpx;
    font-family:MicrosoftYaHei;
    color:rgba(51,51,51,1);
    position: absolute;
    left: 170rpx;
    top: 40rpx;
}

.head>view text{
    line-height: 50rpx;
}

.head>view .phone{
    display: block;
    color:rgba(102,102,102,1);
}

.item{
    width: 710rpx;
    height: 128rpx;
    background: white;
    margin: 0 auto;
    margin-top: 10rpx;
    font-size:26rpx;
    color:rgba(51,51,51,1);
    padding: 0 30rpx;
    box-sizing: border-box;
}

.item>view{
    line-height: 64rpx;
}

.item>view image{
    width: 30rpx;
    height: 32rpx;
    vertical-align: middle;
    margin-right: 20rpx;
}